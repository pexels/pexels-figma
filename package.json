{
  "name": "pexels-figma",
  "version": "1.0.9",
  "description": "A Figma plugin to insert photos from Pexels.",
  "homepage": "https://www.figma.com/community/plugin/829802086526281657/Pexels",
  "license": "ISC",
  "scripts": {
    "build": "/usr/local/bin/node node_modules/.bin/webpack --mode=production",
    "build:watch": "/usr/local/bin/node node_modules/.bin/webpack --mode=development --watch",
    "prettier:format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,json}'"
  },
  "author": "Craig Dennis <craig@simplebitdesign.com> (https://craigmdennis.com)",
  "dependencies": {
    "@closeio/use-infinite-scroll": "^1.0.0",
    "babel-plugin-styled-components": "^1.10.7",
    "purgecss-webpack-plugin": "^2.1.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-intersection-observer": "^8.26.1",
    "react-loading-skeleton": "^2.0.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.9.5",
    "@svgr/webpack": "^5.3.1",
    "@types/node": "^13.13.0",
    "@types/react": "^16.8.24",
    "@types/react-dom": "^16.8.5",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.5.3",
    "figma-plugin-ds": "git+https://github.com/craigmdennis/figma-plugin-ds.git#add-package-json",
    "file-loader": "^6.0.0",
    "html-webpack-inline-source-plugin": "^0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.2",
    "lint-staged": "^9.2.1",
    "prettier": "^1.18.2",
    "style-loader": "^1.2.0",
    "ts-loader": "^6.0.4",
    "tslint": "^5.18.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.7.4",
    "url-loader": "^2.1.0",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,css,json}": [
      "prettier --write",
      "git add"
    ]
  }
}
